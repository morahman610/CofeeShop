@{
    ViewBag.Title = "Home Page";
}


<head>
    <link rel="stylesheet" type="text/css" href="StyleSheet.css">
</head>
<div class="jumbotron">
    <h1>Expresso</h1>
    <p>Sigining up gives you access needed to add, delete, and update the products.</p>
    <p><a class="btn btn-default" href="/Home/Registration">Register &raquo;</a></p>
</div>

<table>
    <tr>
        <th>Name</th>
        <th>Description</th>
        <th>Quantity</th>
        <th>Price</th>
    </tr>

    @for (int i = 0; i < ViewBag.Items.Count; i++)
    {
        <tr>
            <td>@ViewBag.Items[i].Name</td>
            <td>@ViewBag.Items[i].Description</td>
            <td>@ViewBag.Items[i].Quantity</td>
            <td>@ViewBag.Items[i].Price</td>
        </tr>
    }

</table>